        <div class="container">
            <aside>
                <ul>
                  <li><a href="/tbkel6pweb/views/home.ejs"><i class="fas fa-home"></i>&nbsp;Home</a></li>
                  <li><a href="/tbkel6pweb/views/daftarkkn.ejs"><i class="fas fa-list-ul"></i>&nbsp;Pendaftaran KKN</a></li>
                  <li><a href="/tbkel6pweb/views/verifikasi.ejs"><i class="fas fa-list-ul"></i>&nbsp;Verifikasi</a></li>
